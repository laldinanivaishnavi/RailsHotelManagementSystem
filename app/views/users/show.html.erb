<%= stylesheet_link_tag "custom" %>
<% provide(:title, @user.username) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.username %>
        <p class="text-right">
        <%= link_to "Log out", logout_path, method: :delete %>
        </p>
      </h1>
    </section>
  </aside>
</div>
